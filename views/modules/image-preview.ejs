<div class="position-relative overflow-hidden border rounded-4 mx-auto" style="aspect-ratio: 1;max-width: 450px;width: 100%;" onclick="showPreviewImage('<%-fileContent %>')">
    <img src="<%-fileContent %>" style="width:100%;height:100%;object-fit: contain;" loading="lazy">
</div>
<script>
    const showPreviewImage = (path) => {
        new bootstrap.Modal("#previewImageModal").show();
        document.getElementById("preview-image-model-render").src = path;
    }
</script>